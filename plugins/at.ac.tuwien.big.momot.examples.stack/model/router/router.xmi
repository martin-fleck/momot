<?xml version="1.0" encoding="ASCII"?>
<router:Container
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:router="http://router/1.0"
    xsi:schemaLocation="http://router/1.0 router.ecore">
  <agents xsi:type="router:Provider"
      name="s1"
      speed="3"/>
  <agents xsi:type="router:Provider"
      name="s2"
      speed="3"/>
  <agents xsi:type="router:Consumer"
      name="pc1"
      speed="2"/>
  <agents xsi:type="router:Consumer"
      name="pc2"
      speed="2"/>
  <agents xsi:type="router:Consumer"
      name="pc3"
      speed="2"/>
  <devices xsi:type="router:Server"
      name="s1">
    <outgoing
        xsi:type="router:GlassFiberCable"
        speed="4"
        target="//@devices.2"/>
  </devices>
  <devices xsi:type="router:Server"
      name="s2">
    <outgoing
        xsi:type="router:GlassFiberCable"
        speed="4"
        target="//@devices.3"/>
  </devices>
  <devices xsi:type="router:Router"
      incoming="//@devices.0/@outgoing.0"
      name="r1"
      maxSpeed="4">
    <outgoing
        xsi:type="router:CopperCable"
        speed="2"
        target="//@devices.4"/>
  </devices>
  <devices xsi:type="router:Router"
      incoming="//@devices.1/@outgoing.0"
      name="r2"
      maxSpeed="4">
    <outgoing
        xsi:type="router:CopperCable"
        speed="2"
        target="//@devices.5"/>
  </devices>
  <devices xsi:type="router:Computer"
      incoming="//@devices.2/@outgoing.0"
      name="pc1"/>
  <devices xsi:type="router:Computer"
      incoming="//@devices.3/@outgoing.0"
      name="pc3"/>
</router:Container>
